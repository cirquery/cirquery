{
  "name": "cirquery",
  "version": "0.1.0",
  "description": "Core Common/Intermediate Representation for transforming human-friendly query DSLs to backend adapters.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap --clean --out-dir dist",
    "prepublishOnly": "npm run typecheck && npm run build",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write src"
  },
  "repository": { "type": "git", "url": "git+https://github.com/cirquery/cirquery.git" },
  "bugs": { "url": "https://github.com/cirquery/cirquery/issues" },
  "homepage": "https://github.com/cirquery/cirquery#readme",
  "keywords": ["query","dsl","parser","cir","normalization"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "lowdb": "^7.0.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "chevrotain": "^11.0.3"
  }
}
